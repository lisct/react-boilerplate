{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-unused-vars": [
      "error",
      {
        "vars": "local",
        "args": "none"
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "strict": [
      2,
      "never"
    ],
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": [
          "Link"
        ],
        "specialLink": [
          "to"
        ]
      }
    ],
    "react/jsx-filename-extension": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-indent": [
      "error",
      4
    ],
    "indent": [
      "error",
      4
    ],
    "react/jsx-indent-props": [
      "error",
      4
    ]
  },
  "plugins": [
    "react"
  ]
}